{
  "plugins": ["node_modules/jsdoc-babel", "plugins/markdown"],
  "babel": {
    "plugins": ["@babel/plugin-transform-flow-strip-types"]
  },
  "source": {
    "include": [
      "README.md",
      "./src/cloud-code",
      "./src/Options/docs.js",
      "./src/ParseServer.js",
      "./src/Adapters"
    ],
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "templates": {
    "default": {
      "outputSourceFiles": false,
      "showInheritedInNav": false,
      "useLongnameInNav": true
    },
    "cleverLinks": true,
    "monospaceLinks": false
  },
  "opts": {
    "encoding": "utf8",
    "readme": "./README.md",
    "recurse": true,
    "template": "./node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "default_theme": "dark",
      "title": "<img src='../.github/parse-server-logo.png' class='logo'/>",
      "create_style": "header, .sidebar-section-title, .sidebar-title { color: #139cee !important } .logo { margin-left : 40px; margin-right: 40px }"
    }
  },
  "markdown": {
    "hardwrap": false,
    "idInHeadings": true
  }
}
